{
	"$schema": "https://turbo.build/schema.json",
	"pipeline": {
		"build": {
			"dependsOn": ["^build"],
			"inputs": ["src/**", "!src/**/*.spec.ts", "*.json", "typescript/*.json"],
			"outputs": ["out/**", "!out/**/*.spec.js"],
			"outputMode": "new-only"
		},
		"test": {
			"dependsOn": ["build"],
			"inputs": ["src/**/*.spec.ts"],
			"outputs": ["out/**/*.spec.js"],
			"outputMode": "new-only"
		}
	}
}
